  "version": "0.2.0",
  "configurations": [
    {
      "name": "Edge Dev",
      "type": "edge",
      "version": "dev",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "name": "Edge Stable",
      "type": "edge",
      "request": "launch",
      "version": "stable",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
